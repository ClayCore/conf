set nocompatible
set ai
set noexpandtab
set ruler
filetype plugin indent on
set cmdheight=2
set nosmartindent
set tabstop=4
set shiftwidth=4
set nonumber
set laststatus=2
set history=1000
set t_Co=256
set encoding=utf8
set nocursorline
set lazyredraw
set wildmenu
set noshowmatch
colo delek
syntax on

nmap <F2> :w<CR>
imap <F2> <ESC>w<CR>i

